<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="50" prefWidth="400" spacing="40" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.board.config.boardconfiggui.controllers.LabelComboBoxWidgetController">
    <children>
        <Label fx:id="comboBoxLabel" prefWidth="100.0"  wrapText="true">
            <HBox.margin>
                <Insets top="5.0" />
            </HBox.margin>
        </Label>
        <ComboBox prefWidth="150.0" fx:id="cmbInfo" onAction="#comboBoxSelectionChanged"/>
    </children>
    <padding>
        <Insets left="10.0" top="10.0" />
    </padding>
</HBox>
